{
  "name": "magic.github.io",
  "version": "0.0.1",
  "private": true,
  "author": "Wizards & Witches",
  "description": "@magic org github page. generate static pages and serverless lambdas. ~5kb client boilerplate.",
  "license": "AGPL-3.0",
  "homepage": "https://magic.github.io",
  "engines": {
    "node": ">=12.13.0"
  },
  "scripts": {
    "start": "t -p",
    "build": "NODE_ENV=production magic build",
    "prod": "NODE_ENV=production magic build serve --watch src",
    "clean": "magic clean",
    "serve": "magic serve --watch src",
    "dev": "magic dev --watch src",
    "format": "f -w --exclude docs",
    "test": "t --exclude docs example config.js",
    "calls": "calls",
    "connect": "magic connect",
    "publish": "magic publish"
  },
  "dependencies": {
    "@magic-modules/gdpr": "0.0.14",
    "@magic-modules/git-badges": "0.0.4",
    "@magic-modules/library-list": "0.0.7",
    "@magic-modules/light-switch": "0.0.5",
    "@magic-modules/module-list": "0.0.8",
    "@magic-modules/pre": "0.0.6",
    "@magic-modules/theme-list": "0.0.5",
    "@magic-themes/project": "0.0.8",
    "@magic/core": "0.0.55"
  },
  "devDependencies": {
    "@magic/format": "0.0.7",
    "@magic/test": "0.1.37"
  },
  "keywords": [
    "magic",
    "static",
    "serverless",
    "page",
    "generator"
  ],
  "contributors": [
